[submodule ".oh-my-zsh"]
	path = .oh-my-zsh
	url = https://github.com/ohmyzsh/ohmyzsh.git
[submodule ".krew/index/default"]
	path = .krew/index/default
	url = https://github.com/kubernetes-sigs/krew-index.git
[submodule ".config/oh-my-zsh-custom/plugins/zsh-autosuggestions"]
	path = .config/oh-my-zsh-custom/plugins/zsh-autosuggestions
	url = https://github.com/zsh-users/zsh-autosuggestions
[submodule ".config/oh-my-zsh-custom/plugins/zsh-syntax-highlighting"]
	path = .config/oh-my-zsh-custom/plugins/zsh-syntax-highlighting
	url = https://github.com/zsh-users/zsh-syntax-highlighting
[submodule ".config/fastfetch"]
	path = .config/fastfetch
	url = https://github.com/fastfetch-cli/fastfetch.git
